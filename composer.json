{
    "name": "lildude/phpsmug",
    "type": "library",
    "description": "SmugMug API v2 client",
    "homepage": "http://phpsmug.com",
    "keywords": ["smugmug", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Colin Seymour",
            "homepage": "http://colinseymour.co.uk"
        },
        {
            "name": "phpSmug Contributors",
            "homepage": "https://github.com/lildude/phpSmug/graphs/contributors"
        }
    ],
    "require": {
        "php":             ">=5.3.2",
        "ext-curl":        "*",
        "guzzle/guzzle":   "~3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": { "phpSmug\\": "lib/phpSmug" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
